    "mongoose": "^7.6.5",
    "mongodb": "^5.8.0",
    "cors": "^2.8.5",
    "ws": "^8.13.0", 
    "langchain": "^0.1.0",
    "@langchain/openai": "^0.1.0",
    "@langchain/community": "^0.1.0"
  }
}
